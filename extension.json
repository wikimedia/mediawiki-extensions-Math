	"config": {
		"MathDefaultLaTeXMLSetting": {
			"format": "xhtml",
			"whatsin": "math",
			"whatsout": "math",
			"0": "pmml",
			"1": "cmml",
			"2": "mathtex",
			"3": "nodefaultresources",
			"preload": [
				"LaTeX.pool",
				"article.cls",
				"amsmath.sty",
				"amsthm.sty",
				"amstext.sty",
				"amssymb.sty",
				"eucal.sty",
				"[dvipsnames]xcolor.sty",
				"url.sty",
				"hyperref.sty",
				"[ids]latexml.sty",
				"texvc"
			],
			"linelength": 90
		},
		"MathDisableTexFilter": "new",
		"MathEnableExperimentalInputFormats": false,
		"MathLaTeXMLTimeout": 240,
		"MathLaTeXMLUrl": "http://gw125.iu.xsede.org:8888",
		"MathMathMLTimeout": 20,
		"MathMathMLUrl": "https://mathoid-beta.wmflabs.org",
