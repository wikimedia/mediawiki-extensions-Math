			"class": "ResourceLoaderImageModule",
			"selectorWithoutVariant": ".oo-ui-icon-{name}",
			"selectorWithVariant": ".oo-ui-image-{variant}.oo-ui-icon-{name}",
			"variants": {
				"progressive": {
					"color": "#36c"
				},
				"invert": {
					"color": "#fff"
				}
			},
			"images": {
				"math": {
