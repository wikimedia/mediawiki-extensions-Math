			"png",
			"source",
			"mathml"
		],
		"MathEnableWikibaseDataType": true,
		"MathEnableFormulaLinks": false,
		"MathWikibasePropertyIdHasPart": "P527",
		"MathWikibasePropertyIdDefiningFormula": "P2534",
